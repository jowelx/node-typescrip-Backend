{
  "name": "typescript-nodejs-setup",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Efrain Rodriguez <efrainrg92@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".ts,.js\" --out-dir build --copy-files",
    "start": "node build/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules src/index.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "*"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
